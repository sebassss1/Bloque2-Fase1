#Comando usado para crear la OU de los equipos

samba-tool ou create 'OU=ClientsComputers,DC=edu-gva,DC=es' --description='Departamento de computers'
#Comandos usados para crear los equipos dentro de la OU

samba-tool computer create W10IM-01 --computerou='OU=ClientsComputers' --description='Computer dep Direccion' --username=Bmonchob
samba-tool computer create W10IM-2 --computerou='OU=ClientsComputers' --description='Computer dep Direccion' --username=Mjuanm
