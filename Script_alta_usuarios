#Comandos usados para crear los usuarios

samba-tool user create Jalbaj logiPassword7 --must-change-at-next-login --userou='ou=Conserjeria' --home-directory=/home/Johan --given-name="Johan" --surname="Alba" --mail=JOHAN.ALBA@iesmiralcamp.es --login-shell=/bin/bash
